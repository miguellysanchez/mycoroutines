<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/cbuilders_button_launch_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launch Demo" />
    <Button
        android:id="@+id/cbuilders_button_launch_error_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launch with Error Demo" />

    <Button
        android:id="@+id/cbuilders_button_async_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Async Demo" />

    <Button
        android:id="@+id/cbuilders_button_async_error_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Async with Error Demo" />

    <Button
        android:id="@+id/cbuilders_button_run_blocking_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Run Blocking Demo" />


    <Button
        android:id="@+id/cbuilders_button_run_blocking_properly_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Run Blocking Properly Demo" />

</LinearLayout>